{
  "name": "angular2-iot",
  "version": "0.0.1",
  "description": "IoT support for Angular 2",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "tslint src/**/*.ts && tsc",
    "example:build": "rimraf examples/dist && tsc -outDir examples/dist -experimentalDecorators examples/blink.ts typings/main.d.ts",
    "example:run": "node examples/dist/examples/blink",
    "typings": "typings install",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urish/angular2-iot.git"
  },
  "keywords": [
    "angular2",
    "iot",
    "arduino",
    "robotics",
    "raspberry pi"
  ],
  "author": "Uri Shaked",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/urish/angular2-iot/issues"
  },
  "homepage": "https://github.com/urish/angular2-iot#readme",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "angular2": "2.0.0-beta.15",
    "angular2-universal": "0.98.3",
    "es6-shim": "0.35.0",
    "johnny-five": "0.9.44",
    "preboot": "2.0.6",
    "reflect-metadata": "0.1.2",
    "rimraf": "2.5.2",
    "rxjs": "5.0.0-beta.2",
    "tslint": "3.8.1",
    "typescript": "1.8.10",
    "typings": "0.8.1",
    "zone.js": "0.6.12"
  },
  "peerDependencies": {
    "angular2": "^2.0.0-beta.12",
    "angular2-universal": "^0.90.2",
    "johnny-five": "^0.9.0"
  }
}
